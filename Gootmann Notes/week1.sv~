module Light(f, s0, s1, w0, w1, w2, w3);
   input s0, s1, w0, w1, w2, w3;
   output f;
   assign f = (~s0 & w0 & ~s1) | (s0 & w1 & ~s1) | (~s0 & w2 & s1) | (s0 & w0 & s1);

endmodule // Light

module Light_testbench();
   reg s0, s1, w0, w1, w2, w3;
   wire f;

